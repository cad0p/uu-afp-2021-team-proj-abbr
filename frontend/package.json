{
  "name": "shorthndr",
  "displayName": "ShortHndr",
  "repository": "https://github.com/cad0p/uu-afp-2021-team-proj-abbr",
  "version": "1.0.0",
  "description": "This is the integration of the ShortHndr with VS Code. Implemented in [`Elm`](https://elm-lang.org/).",
  "categories": [
    "Formatters",
    "Other",
    "Education"
  ],
  "main": "extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "shorthndr.ping",
        "title": "ShortHndr: Ping"
      }
    ]
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "scripts": {
    "postinstall": "npm run make:elm",
    "make:elm": "elm make ./src/ExpandWorker.elm --output ./dist/ExpandWorker.js --debug",
    "format:elm": "elm-format --yes ./src/*.elm "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/vscode": "^1.32.0",
    "elm": "^0.19.1-5",
    "elm-format": "^0.8.5"
  }
}
