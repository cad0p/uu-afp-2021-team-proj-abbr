{
  "name": "shorthndr",
  "displayName": "ShortHndr",
  "version": "1.0.0",
  "description": "This is the integration of the ShortHndr with VS Code. Implemented in [`Elm`](https://elm-lang.org/).",
  "categories": [
    "Formatters",
    "Other",
    "Education"
  ],
  "main": "extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "shorthndr.ping",
        "title": "ShortHndr: Ping"
      }
    ]
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "scripts": {
    "preinstall": "npm run make:elm",
    "make:elm": "elm make ./src/ExpandWorker.elm --output ./dist/ExpandWorker.js --debug",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "generator-code": "^1.6.9",
    "yo": "^4.3.0",
    "@types/vscode": "^1.32.0"
  }
}
